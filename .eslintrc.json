{
    "extends": "standard",
    "overrides": [
      {
        "files": ["*spec.js", "test/**/*.js"],
        "rules": {
          "no-unused-expressions": "off",
          "no-unused-vars": "off"
        },
        "globals": {
          "describe": false,
          "beforeEach": false,
          "afterEach": false,
          "beforeAll": false,
          "afterAll": false,
          "before": false,
          "after": false,
          "expect": false,
          "jest": false,
          "it": false,
          "__rewire_reset_all__": false
        }
      }
    ]
}
